<div class="container-fluid">
    <div class="row">
        <div class="col-sm-7">
            <table class="table table-striped latest-data">
                <tbody>
                {{#proxied-each
                  keyForId="id"
                  content=model.databases
                  as |db|
                }}
                  {{mdbmon-database db=db.content}}
                {{/proxied-each}}
                </tbody>
            </table>
        </div>
        <div class="col-sm-5">
            <h3>Adjust Timeout</h3>
          {{input value=timeout}}

            <h3>Adjust Rows</h3>
            {{input value=numRows}}
        </div>
    </div>
</div>
