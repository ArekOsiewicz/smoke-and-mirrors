{
  "name": "smoke-and-mirrors",
  "version": "0.4.4",
  "description": "Performance minded `views`, `mixins`, and `components` to help you deliver your ambitions",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "ember build",
    "test": "ember try:testall",
    "lint": "eslint -c .eslintrc addon/",
    "lint-all": "eslint -c .eslintrc app/ addon/ tests/",
    "lint-staged": "git diff --diff-filter=ACMRTUXB --cached --name-only | grep '*.js' | grep -v 'node_modules' | grep -v 'tests' | xargs eslint -c .eslintrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runspired/smoke-and-mirrors"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "homepage": "https://runspired.github.io/smoke-and-mirrors",
  "bugs": "https://github.com/runspired/smoke-and-mirrors/issues",
  "author": {
    "name": "Chris Thoburn (@runspired)",
    "url": "http://runspired.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "broccoli-asset-rev": "^2.1.2",
    "ember-cli": "1.13.8",
    "ember-cli-app-version": "0.5.0",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-eslint": "1.2.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "^1.0.3",
    "ember-cli-release": "0.2.3",
    "ember-cli-uglify": "^1.0.1",
    "ember-code-snippet": "^1.1.0",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.0",
    "ember-export-application-global": "^1.0.3",
    "ember-run-raf": "^1.0.5",
    "ember-suave": "1.2.2",
    "ember-try": "0.0.6",
    "eslint": "^1.9.0",
    "eslint-config-ember": "^0.2.0",
    "grunt": "0.4.5",
    "grunt-bump": "0.0.16",
    "grunt-cli": "0.1.13",
    "jit-grunt": "0.9.0",
    "js-string-escape": "^1.0.0",
    "time-grunt": "1.0.0"
  },
  "keywords": [
    "ember-addon",
    "occlusion",
    "infinite",
    "infinite-scroll",
    "collection",
    "grid",
    "list-view",
    "recycling",
    "cloaking",
    "performance",
    "lists"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-async-image": "0.0.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
